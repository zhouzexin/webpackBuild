{
    "presets": [
      "es2015",
      "react",
      "stage-1",
      "stage-2",
      "stage-3",
    ],
    "plugins": [
        "transform-runtime",
        "transform-react-jsx",
        "syntax-dynamic-import",
        "transform-async-to-module-method",
        "transform-class-properties",
        "transform-decorators",
        "transform-function-bind"
    ]
}
