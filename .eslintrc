{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": [0],
        "quotes": [0, "double"],
        "no-console": 0
    },
    env : {
        browser : true,
        node : true,
        es6 : true
    },
    "extends": [
        "eslint:recommended"
    ]
}
